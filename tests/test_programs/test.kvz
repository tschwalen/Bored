
function do_something (n) {
    return (n - 1) * (n - 1);
}

function main () {
    var x = 5.0;
    print (   
        do_something(x)
    );
}

function greatest_common_factor (a, b) {   
    while (a != b) do {
        if (a > b) then {
            a = a - b;
        }   
        else {
            b = b - a;
        }
    }
    return a;
}

function is_even (n) {
    return n % 2 == 0;
}